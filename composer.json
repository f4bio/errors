{
    "name": "laravelcollective/errors",
    "description": "Error pages for the Laravel Framework",
    "license": "MIT",
    "homepage": "https://laravelcollective.com",
    "support": {
        "issues": "https://github.com/LaravelCollective/errors/issues",
        "source": "https://github.com/LaravelCollective/errors"
    },
    "authors": [
        {
            "name": "Matt Lantz",
            "email": "matt@laravelcollective.com"
        }
    ],
    "require": {
        "php": ">=8.1",
        "laravel/framework": "^8.0|^9.0"
    },
    "autoload": {
        "psr-4": {
            "Collective\\Errors\\": "src/"
        }
    },
    "extra": {
